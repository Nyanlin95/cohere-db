    "schema",
    "ai",
    "claude",
    "cursor",
    "windsurf",
    "context",
    "documentation",
    "postgresql",
    "mysql",
    "sqlite",
    "mongodb",
    "firebase",
    "prisma",
    "drizzle",
    "orm",
    "sql"
