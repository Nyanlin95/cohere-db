  "author": "Nyan Lin Maung <nyanlinmaung.d@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Nyanlin95/cohere-db"
  },
  "bugs": {
    "url": "https://github.com/Nyanlin95/cohere-db/issues"
  },
  "homepage": "https://github.com/Nyanlin95/cohere-db#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist",
    "templates",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
