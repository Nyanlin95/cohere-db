    "firebase-admin": "^12.0.0",
    "inquirer": "^9.2.0",
    "js-yaml": "^4.1.0",
    "mongodb": "^6.0.0",
    "mysql2": "^3.16.3",
    "pg": "^8.11.0",
    "sqlite3": "^5.1.7"
